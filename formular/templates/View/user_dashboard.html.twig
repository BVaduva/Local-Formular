{% extends 'base.twig' %}

{% block title %}
	Dashboard
{% endblock %}

{% block content %}
	<div class="row">
		<div class="col-md-3">
			{% include 'form.twig' with {
                'form_title': form_title,
                'form_action': form_action,
                'form_method': form_method,
                'input_data': input_data,
                'buttons': buttons,
            } %}
		</div>
		<div class="col-md-6" id="accordion-dashboard">
			{% include 'card_with_tabs.twig' with {
                'tabs': card
            } %}
			<br>
			{% include 'accordion.twig' with {
                'items': items.items
            } %}

		</div>
		<div class="col-md-3">
			{% include 'carousel.twig' with {
                'slides': carousel,
                'autoplay': true,
            } %}
		</div>
	</div>
{% endblock %}
